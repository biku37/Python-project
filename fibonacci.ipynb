    "def fibonacci(n):\n",
    "    if n ==1:\n",
    "        return 1\n",
    "    elif n==2:\n",
    "        return 1\n",
    "    elif n>2:\n",
    "        return fibonacci(n-1)+ fibonacci(n-2)\n",
    "\n",
    "a=int(input(print(\"enter the position number of the series: \")))\n",
    "\n",
    "for n in range(1,a+1):\n",
    "    print(n, \":\", fibonacci(n))"
   ]
